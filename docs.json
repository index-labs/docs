{
   "$schema": "https://mintlify.com/docs.json",
   "theme": "mint",
   "name": "Devv Documentation",
   "colors": {
      "primary": "#16A34A",
      "light": "#07C983",
      "dark": "#15803D"
   },
   "logo": {
      "light": "/logo/logo-light.svg",
      "dark": "/logo/logo-dark.svg"
   },
   "favicon": {
      "dark": "/favicon-dark.png",
      "light": "/favicon-light.png"
   },
   "navigation": {
      "tabs": [
         {
            "tab": "Guides",
            "groups": [
               {
                  "group": "Introduction",
                  "pages": ["introduction", "FAQ", "getting-started"]
               }
            ]
         },
         {
            "tab": "Integrations",
            "groups": [
               {
                  "group": "Introduction",
                  "pages": ["/integrations/introduction"]
               },
               {
                  "group": "Native Integrations",
                  "pages": [
                     "/integrations/devv-database",
                     "/integrations/authentication",
                     "/integrations/web-reader",
                     "/integrations/web-search",
                     "/integrations/devv-email",
                     "/integrations/llm"
                  ]
               },
               {
                  "group": "Third-party Integration",
                  "pages": [
                     "/integrations/openrouter",
                     "/integrations/elevenlabs",
                     "/integrations/replicate"
                  ]
               }
            ]
         }
      ],
      "global": {
         "anchors": [
            {
               "anchor": "Documentation",
               "href": "https://docs.devv.ai/introduction",
               "icon": "book-open-cover"
            },
            {
               "anchor": "Community",
               "href": "https://discord.gg/BxNTkUzEnG",
               "icon": "discord"
            }
         ]
      }
   },
   "navbar": {
      "links": [
         {
            "label": "Support",
            "href": "mailto:support@devv.ai"
         }
      ],
      "primary": {
         "type": "button",
         "label": "Go to App",
         "href": "https://devv.ai"
      }
   },
   "footer": {
      "socials": {
         "x": "https://x.com/devv_ai",
         "github": "https://github.com/index-labs/docs"
      }
   }
}
